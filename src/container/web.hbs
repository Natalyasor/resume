{{!--
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<div>
	<main>
		<section>
			<ul>
				<h3>WEB</h3>
				{{#with web}}
				{{#each languages}}
				<li><strong>{{name}}</strong></li>
				<strong>{{version}}</strong>
				{{#if version}} (version ){{/if}}
				<br>
				<p>{{description}}</p>

				{{#each elements}}
				<strong>{{name}}</strong>
				<br>
				<p>{{description}}</p>

				{{#each attributes}}
				<strong>{{name}}</strong>
				<p>{{description}}</p>
				{{/each}}
				{{/each}}

				{{#each properties}}
				<li><strong>{{name}}</strong></li>
				<p>{{description}}</p>
				<br>

				{{#each values}}
				<li><strong>{{value}}</strong></li>
				<br>
				<p>{{description}}</p>
				{{/each}}
				{{/each}}

				{{#each functions}}
				<li><strong>{{name}}</strong></li>
				<br>
				<p>{{description}}</p>

				{{#each parameters}}
				<li><strong>{{name}} ({{type}})</strong></li>
				<p>{{description}}</p>
				{{/each}}
				{{/each}}
				{{/each}}

				<br>
				{{/with}}

			</ul>
		</section>
	</main>
</div>

{{!--
Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
Адже наш layout/default.hbs має вже структуру HTML
--}}